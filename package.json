{
  "name": "sitepath",
  "version": "0.2.0",
  "description": "A self-describing URL path-like thing",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha tests/*.js",
    "karma": "./node_modules/.bin/karma start",
    "karma:travis": "./node_modules/.bin/karma start travis.conf.js",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha tests/*.js",
    "travis": "npm run test && npm run karma:travis",
    "docs": "./node_modules/.bin/documentation index.js -f md -o docs.md"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:rm3web/sitepath.git"
  },
  "bugs": {
    "url": "https://github.com/rm3web/sitepath/issues"
  },
  "homepage": "https://github.com/rm3web/sitepath",
  "author": "Wirehead <wh-npm@wirewd.com>",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "chai": "^3.4.1",
    "documentation": "^3.0.4",
    "istanbul": "^0.4.1",
    "karma": "^0.13.15",
    "karma-browserify": "^4.4.2",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-safari-launcher": "^0.1.1",
    "karma-sauce-launcher": "^0.3.0",
    "mocha": "^2.3.4",
    "phantomjs": "^1.9.19"
  }
}
